{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s.%(msecs)03d %(levelname)s %(module)s: %(message)s"
        },
        "testSimple": {
            "format": "%(levelname)s: %(module)s: %(message)s"
        },
        "functionNames": {
            "format": "%(asctime)s.%(msecs)03d %(module)s.%(funcName)s():%(lineno)d %(message)s",
            "datefmt" : "%H:%M:%S"
        },
        "bestFormat": {
            "format": "%(asctime)s.%(msecs)03d %(levelname)-6s %(name)-15s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "functionNames",
            "stream": "ext://sys.stdout"
        },
        "rotatingFileHandler": {
            "class": "logging.handlers.RotatingFileHandler",
            "formatter": "bestFormat",
            "filename": "/tmp/uml-extensions.log",
            "mode": "a",
            "maxBytes": 163840,
            "backupCount": 5,
            "encoding": "utf-8"
        }

    },
    "loggers": {
        "tests": {
            "level": "INFO",
            "handlers": ["consoleHandler"],
            "propagate": false
        },
        "umlextensions": {
            "level": "WARNING",
            "handlers": ["consoleHandler"],
            "propagate": true
        },
        "umlextensions.tools": {
            "level": "WARNING",
            "handlers": ["consoleHandler"],
            "propagate": true
        },
        "codeallybasic.UnitTestBase": {
            "level": "WARNING",
            "propagate": false
        },
        "umlextensions.ExtensionsManager": {
            "level": "ERROR",
            "handlers": ["rotatingFileHandler"],
            "propagate": false
        },
        "umlextensions.tools.sugiyama": {
            "level": "INFO",
            "handlers": ["consoleHandler"],
            "propagate": false
        }
    }
}
